NameVirtualHost *

<VirtualHost *>
        ServerName <%= servername %>
        ServerAlias <%= serveralias %>

        DocumentRoot <%= root %>
        DirectoryIndex <%= index %>

        CustomLog /var/log/apache2/<%= servername %>_access.log combined
        ErrorLog /var/log/apache2/<%= servername %>_error.log
        
        <%= customcontent %>

</VirtualHost>